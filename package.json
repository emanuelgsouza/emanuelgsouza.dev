{
  "name": "emanuelgsouza-11ty",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@quasibit/eleventy-plugin-sitemap": "^2.1.4",
    "@sardine/eleventy-plugin-tinycss": "^1.5.0",
    "dotenv": "^10.0.0",
    "eleventy-plugin-code-style-hooks": "^2.0.0",
    "eslint": "7",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "5",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "sass": "^1.35.2",
    "storyblok-11ty": "^1.6.0",
    "storyblok-js-client": "^4.1.3",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0"
  },
  "scripts": {
    "clean": "rm -rf _site/",
    "serve": "ELEVENTY_ENV=dev eleventy --serve",
    "watch:sass": "npx sass src/sass:_site/css --watch",
    "build:css": "npx sass src/sass:_site/css",
    "start": "yarn serve & yarn watch:sass",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "lint:styles": "npx stylelint '**/*.scss'",
    "build": "yarn clean && yarn build:css && ELEVENTY_ENV=prod npx eleventy",
    "lint:js": "eslint ./**/*.js --fix"
  },
  "dependencies": {
    "bulma": "^0.9.3",
    "dayjs": "^1.10.6"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
